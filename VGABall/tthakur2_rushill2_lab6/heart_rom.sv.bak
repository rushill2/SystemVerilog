module heart_rom ( input [10:0]	addr,
						output [15:0]	data
					 );

	parameter ADDR_WIDTH = 11;
   parameter DATA_WIDTH =  16;
	logic [ADDR_WIDTH-1:0] addr_reg;
        parameter [0:2**ADDR_WIDTH-1][DATA_WIDTH-1:0] ROM = {	
        8'b0000000000000000, // 0
        8'b0000000000000000, // 1
        8'b0011111001111100, // 2
        8'b0111111111111110, // 3
        8'b0111111111111110, // 4
        8'b0111111111111110, // 5
        8'b0011111111111100, // 6
        8'b0001111111111000, // 7
        8'b0000111111110000, // 8
        8'b0000011111100000, // 9
        8'b0000001111000000, // a
        8'b0000000110000000, // b
        8'b0000000000000000, // c
        8'b0000000000000000, // d
        8'b0000000000000000, // e
        8'b0000000000000000, // f
        };

	assign data = ROM[addr];

endmodule  